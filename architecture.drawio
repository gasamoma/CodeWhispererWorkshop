        <mxCell id="SCVMTBWpLvtzJIhz15lM-6" value="&lt;p style=&quot;margin: 0cm; font-size: 12pt; font-family: Calibri, sans-serif;&quot; class=&quot;MsoNormal&quot;&gt;The User enters the application via web or mobile&lt;/p&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;fontColor=#000000;" parent="jZVaUwOg5UY3L9NAnvdq-71" vertex="1">
