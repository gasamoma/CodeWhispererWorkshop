        </mxCell>
        <mxCell id="SCVMTBWpLvtzJIhz15lM-9" value="&lt;p style=&quot;margin: 0cm; font-size: 12pt; font-family: Calibri, sans-serif;&quot; class=&quot;MsoNormal&quot;&gt;Amazon Cognito triggers a Post Authentication Event that invokes a lambda function&lt;/p&gt;" style="text;html=1;align=left;verticalAlign=top;spacingTop=-4;fontSize=14;labelBackgroundColor=none;whiteSpace=wrap;fontColor=#000000;" parent="jZVaUwOg5UY3L9NAnvdq-71" vertex="1">
